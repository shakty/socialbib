@article{citeulike:416650,
    abstract = {
A real-valued function \$z(A)\$ defined over subsets \$A\subset
N=\{1,\cdots,n\}\$ is said to be submodular if \$z(A)+z(B)\geq z(A\cup
B)+z(A\cap B)\$. The submodular set covering problem (Q) is to find
\$Z=\min\_{S\subset N}\{\sum\_{j\in S}f\_j\colon z(S)=z(N)\}\$, where \$z\$ is a
nondecreasing submodular set function and \$f\_j\&gt;0\$ for all \$j\$. This problem
is a generalization of the usual 0, 1-integer covering problem, and the
problem of obtaining a minimum weight spanning set of a matroid. The author
discusses a greedy heuristic for problem (Q). It begins with \$S^0=\varphi\$.
The \$(t+1)\$th step begins with the set \$S^t\$. If \$z(S^t)=z(N)\$, stop,
\$Z^G=z(S^t)\$ is the approximate value of \$Z\$ obtained by the heuristic. If
\$z(S^t)\neq z(N)\$, let \$j=j\_t\$ minimize \$(f\_j|(z(S^t\cup\{j\})-z(S^t)))\$
over \$j\in N\backslash S^t\$. Set \$S^{t+1}=S^t\cup\{j\_t\}\$, and continue.
The author obtains bounds for the performance ratio \$Z^G/Z\$. In particular,
if \$z\$ is integer-valued, he shows that \$(Z^G/Z)\leq\sum\_{i=1}^d(1/i)\$,
where \$d=\text{Max}\{z\{j\}-z(\varphi)\colon j\in N\}\$. The author also
shows that there is no polynomially bounded algorithm based on looking at
function values \$z(S)\$ for some \$S\$, which can guarantee a performance
ratio less than a prescribed finite value.},
    author = {Wolsey, L.A.},
    journal = {Combinatorica},
    keywords = {combinatorialoptimization, latticeprogramming, submodularity},
    mrnumber = {MR708153},
    number = {4},
    pages = {385--393},
    title = {{An analysis of the greedy algorithm for the submodular set covering problem}},
    volume = {2},
    year = {1982}
}

